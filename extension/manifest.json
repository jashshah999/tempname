{
  "manifest_version": 3,
  "name": "MSME Flow Quote Generator",
  "version": "1.0",
  "description": "Generate quotes and Excel files directly from your browser",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }]
}